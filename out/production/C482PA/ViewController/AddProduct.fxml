<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="500.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ViewController.AddProduct">
   <children>
      <TableView fx:id="partSelectionTableview" layoutX="420.0" layoutY="68.0" prefHeight="150.0" prefWidth="360.0">
        <columns>
          <TableColumn fx:id="partSelectionIDColumn" prefWidth="68.0" text="Part ID" />
          <TableColumn fx:id="partSelectionNameColumn" prefWidth="82.0" text="Part Name" />
            <TableColumn fx:id="partSelectionInvColumn" prefWidth="93.0" text="Inventory level" />
            <TableColumn fx:id="partSelectionPriceColumn" prefWidth="116.0" text="Price per Unit" />
        </columns>
      </TableView>
      <TableView fx:id="productPartsTableview" layoutX="420.0" layoutY="262.0" prefHeight="150.0" prefWidth="360.0">
        <columns>
          <TableColumn fx:id="productPartIDColumn" prefWidth="68.0" text="Part ID" />
          <TableColumn fx:id="productPartNameColumn" prefWidth="82.0" text="Part Name" />
            <TableColumn fx:id="productPartInvColumn" prefWidth="93.0" text="Inventory Level" />
            <TableColumn fx:id="productPartPriceColumn" prefWidth="116.0" text="Price per Unit" />
        </columns>
      </TableView>
      <TextField fx:id="productIDTxtbox" alignment="CENTER" disable="true" layoutX="100.0" layoutY="143.0" prefWidth="100.0" promptText="Auto GenID#" />
      <TextField fx:id="productNameTxtbox" layoutX="100.0" layoutY="183.0" prefWidth="100.0" />
      <TextField fx:id="productInvTxtbox" layoutX="100.0" layoutY="223.0" prefWidth="75.0" />
      <TextField fx:id="productPriceTxtbox" layoutX="100.0" layoutY="263.0" prefWidth="75.0" />
      <TextField fx:id="productMaxTxtField" layoutX="100.0" layoutY="303.0" prefWidth="75.0" />
      <TextField fx:id="productMinTxtbox" layoutX="245.0" layoutY="303.0" prefWidth="75.0" />
      <TextField fx:id="productSearchTxtbox" layoutX="493.0" layoutY="33.0" />
      <Button fx:id="addProductSearchButton" layoutX="420.0" layoutY="33.0" mnemonicParsing="false" onMouseClicked="#addProductSearchButton" text="Search">
         <font>
            <Font name="Arial Bold" size="12.0" />
         </font></Button>
      <Button fx:id="addProductAddButton" layoutX="739.0" layoutY="227.0" mnemonicParsing="false" onMouseClicked="#addProductAddButton" text="Add">
         <font>
            <Font name="Arial Bold" size="12.0" />
         </font></Button>
      <Button fx:id="addProductCancelButton" layoutX="726.0" layoutY="455.0" mnemonicParsing="false" onMouseClicked="#addProductCancelButton" text="Cancel">
         <font>
            <Font name="Arial Bold" size="12.0" />
         </font></Button>
      <Button fx:id="addProductSaveButton" layoutX="672.0" layoutY="455.0" mnemonicParsing="false" onMouseClicked="#addProductSaveButton" text="Save">
         <font>
            <Font name="Arial Bold" size="12.0" />
         </font></Button>
      <Button fx:id="addProductDeleteButton" layoutX="727.0" layoutY="420.0" mnemonicParsing="false" onMouseClicked="#addProductDeleteButton" text="Delete">
         <font>
            <Font name="Arial Bold" size="12.0" />
         </font></Button>
      <Label layoutX="30.0" layoutY="35.0" text="Add Product">
         <font>
            <Font name="Arial Bold" size="18.0" />
         </font></Label>
      <Label layoutX="35.0" layoutY="143.0" text="ID">
         <font>
            <Font name="Arial" size="14.0" />
         </font></Label>
      <Label layoutX="35.0" layoutY="183.0" text="Name">
         <font>
            <Font name="Arial" size="14.0" />
         </font></Label>
      <Label layoutX="35.0" layoutY="223.0" text="Inv">
         <font>
            <Font name="Arial" size="14.0" />
         </font></Label>
      <Label layoutX="35.0" layoutY="263.0" text="Price">
         <font>
            <Font name="Arial" size="14.0" />
         </font></Label>
      <Label layoutX="35.0" layoutY="303.0" text="Max">
         <font>
            <Font name="Arial" size="14.0" />
         </font></Label>
      <Label layoutX="200.0" layoutY="303.0" text="Min">
         <font>
            <Font name="Arial" size="14.0" />
         </font></Label>
   </children>
</AnchorPane>
